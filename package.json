{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "--- emulators ---": "",
    "emulators:start-firestore": "firebase emulators:start --only firestore",
    "--- start ---": "",
    "start": "ng serve",
    "start:emulator": "ng serve --configuration=emulator",
    "--- cypress ---": "",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:emulate": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" npm run cy:open",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "test:emulator": "ng test --configPath=jest-emulator.config.js",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier:write": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.0",
    "@angular/common": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/fire": "6.0.3",
    "@angular/forms": "10.2.0",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/router": "10.2.0",
    "firebase": "7.24.0",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.1",
    "@angular-devkit/build-angular": "0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "26.0.15",
    "@types/node": "12.12.47",
    "codelyzer": "6.0.1",
    "cross-env": "7.0.2",
    "cypress": "5.4.0",
    "cypress-firebase": "1.5.0",
    "firebase-admin": "9.3.0",
    "firebase-tools": "8.13.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "26.6.1",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
