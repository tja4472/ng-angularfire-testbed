{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:emulator": "ng serve --configuration=emulator",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "--- firestore rules ---": "",
    "pretest-firestore-rules:emulator": "tsc test-security-rules/firestore.rules.spec.ts",
    "test-firestore-rules:emulator": "firebase emulators:exec --only firestore \"jasmine test-security-rules/firestore.rules.spec.js\"",
    "pretest-firestore-rules": "tsc test-security-rules/firestore.rules.spec.ts",
    "test-firestore-rules": "jasmine test-security-rules/firestore.rules.spec.js",
    "--- emulators ---": "",
    "emulators:start": "firebase emulators:start --only firestore",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/fire": "6.0.0",
    "@angular/forms": "9.1.11",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "firebase": "7.15.1",
    "rxjs": "6.5.5",
    "tslib": "1.13.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "9.1.9",
    "@angular/compiler-cli": "9.1.11",
    "@firebase/testing": "0.20.1",
    "@types/node": "12.12.47",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "codelyzer": "5.2.2",
    "firebase-tools": "8.4.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.0.3",
    "karma-jasmine-html-reporter": "1.5.4",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  }
}
