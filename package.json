{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "--- emulators ---": "",
    "emulators:start-real": "firebase emulators:start --only firestore,auth",
    "emulators:start-demo": "firebase emulators:start --only firestore,auth --project demo-1",
    "--- start ---": "",
    "start": "ng serve",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "start:emulator-real": "ng serve --configuration=emulator-real",
    "start:production": "ng serve --configuration=production",
    "--- cypress ---": "",
    "cypress:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run",
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --runInBand",
    "test-all-emulator": "jest --runInBand --group=emulator-required",
    "test-all-no-emulator": "jest --group=-emulator-required",
    "lint": "ng lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.0.3",
    "@angular/common": "13.0.3",
    "@angular/compiler": "13.0.3",
    "@angular/core": "13.0.3",
    "@angular/fire": "7.2.0",
    "@angular/forms": "13.0.3",
    "@angular/platform-browser": "13.0.3",
    "@angular/platform-browser-dynamic": "13.0.3",
    "@angular/router": "13.0.3",
    "firebase": "9.6.3",
    "rxfire": "6.0.3",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.4",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.0.4",
    "@angular/compiler-cli": "13.0.3",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "cross-env": "7.0.3",
    "cypress": "9.3.1",
    "cypress-firebase": "2.2.1",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "firebase-admin": "10.0.1",
    "firebase-tools": "10.1.1",
    "jest": "27.4.7",
    "jest-preset-angular": "11.0.1",
    "jest-runner-groups": "2.1.0",
    "prettier": "2.5.1",
    "ts-snippet": "5.0.2",
    "typescript": "4.4.4"
  }
}
