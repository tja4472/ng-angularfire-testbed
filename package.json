{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
        
    "ng": "ng",
    "start": "ng serve",
    "start:emulator": "ng serve --configuration=emulator",    
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "--- firestore rules ---": "",
    "pretest-firestore-rules:emulator": "tsc test-security-rules/firestore.rules.spec.ts",      
    "test-firestore-rules:emulator": "firebase emulators:exec --only firestore \"jasmine test-security-rules/firestore.rules.spec.js\"",   
    "pretest-firestore-rules": "tsc test-security-rules/firestore.rules.spec.ts",     
    "test-firestore-rules": "jasmine test-security-rules/firestore.rules.spec.js",    
    "--- emulators ---": "",    
    "emulators:start": "firebase emulators:start --only firestore",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/fire": "6.0.0",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "firebase": "7.15.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@firebase/testing": "0.20.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "firebase-tools": "8.4.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "~3.8.3"
  }
}
