{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:emulator": "ng serve --configuration=emulator",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "--- firestore rules ---": "",
    "pretest-firestore-rules:emulator": "tsc test-security-rules/firestore.rules.spec.ts",
    "test-firestore-rules:emulator": "firebase emulators:exec --only firestore \"jasmine test-security-rules/firestore.rules.spec.js\"",
    "pretest-firestore-rules": "tsc test-security-rules/firestore.rules.spec.ts",
    "test-firestore-rules": "jasmine test-security-rules/firestore.rules.spec.js",
    "--- emulators ---": "",
    "emulators:start": "firebase emulators:start --only firestore",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "--- cypress ---": "",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:emulate": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" npm run cy:open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.1",
    "@angular/common": "10.0.1",
    "@angular/compiler": "10.0.1",
    "@angular/core": "10.0.1",
    "@angular/fire": "6.0.2",
    "@angular/forms": "10.0.1",
    "@angular/platform-browser": "10.0.1",
    "@angular/platform-browser-dynamic": "10.0.1",
    "@angular/router": "10.0.1",
    "firebase": "7.15.5",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.0",
    "@angular/cli": "10.0.0",
    "@angular/compiler-cli": "10.0.1",
    "@firebase/testing": "0.20.5",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.47",
    "codelyzer": "5.2.2",
    "cross-env": "7.0.2",
    "cypress": "4.9.0",
    "cypress-firebase": "1.3.0",
    "firebase-admin": "8.13.0",
    "firebase-tools": "8.4.3",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.5"
  }
}
