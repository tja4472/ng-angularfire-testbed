{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "--- emulators ---": "",
    "emulators:start-real": "firebase emulators:start --only firestore,auth",
    "emulators:start-demo": "firebase emulators:start --only firestore,auth --project demo-1",
    "--- start ---": "",
    "start": "ng serve",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "start:emulator-real": "ng serve --configuration=emulator-real",
    "start:production": "ng serve --configuration=production",
    "--- cypress ---": "",
    "cypress:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "test:emulator": "ng test --configPath=jest-emulator.config.js",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier:write": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/fire": "6.1.5",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/router": "12.2.7",
    "firebase": "8.9.0",
    "rxjs": "6.6.3",
    "tslib": "2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.2",
    "@angular-devkit/build-angular": "12.2.7",
    "@angular/cli": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "27.0.2",
    "@types/node": "12.12.47",
    "codelyzer": "^6.0.0",
    "cross-env": "7.0.3",
    "cypress": "8.5.0",
    "cypress-firebase": "2.0.0",
    "firebase-admin": "9.11.1",
    "firebase-tools": "9.18.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "27.2.3",
    "prettier": "2.4.1",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}